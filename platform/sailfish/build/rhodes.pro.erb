QT += core gui network multimedia webkit
<%= @enableDBUS %>

message(Qt version: $$[QT_VERSION])

CONFIG += auroraapp auroraapp_i18n c++14
DEFINES += OS_SAILFISH OS_LINUX

TARGET = <%= @nameApp %>
TEMPLATE = app


CONFIG += warn_on
CONFIG += <%= @buildMode %>

INCLUDEPATH += <%= @rhoRoot %>/platform/shared\
<%= @rhoRoot %>/platform/shared/ruby\
